{
  "configurations": [
    {
      "name": "Mac",
      "compilerPath": "/opt/homebrew/bin/g++-13",
      "compilerArgs": [
        "-Wall",
        "-Wextra",
        "-Wshadow",
        "-O2",
        "-std=c++17",
        "-pedantic",
        "-Wformat=2",
        "-Wfloat-equal",
        "-Wconversion",
        "-Wlogical-op",
        "-Wshift-overflow=2",
        "-Wduplicated-cond",
        "-Wcast-qual",
        "-Wcast-align",
        "-D_GLIBCXX_DEBUG",
        "-D_GLIBCXX_DEBUG_PEDANTIC",
        "-D_FORTIFY_SOURCE=2",
        "-fno-sanitize-recover",
        "-fstack-protector"
      ],
      "includePath": ["${workspaceFolder}/**"],
      "defines": [],
      "macFrameworkPath": [
        "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
      ],
      "cStandard": "c17",
      "cppStandard": "c++17",
      "intelliSenseMode": "macos-gcc-arm64"
    }
  ],
  "version": 4
}
